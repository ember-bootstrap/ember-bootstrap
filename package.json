{
  "name": "ember-bootstrap",
  "version": "4.8.1",
  "description": "Bootstrap components for Ember.js",
  "keywords": [
    "ember-addon",
    "bootstrap"
  ],
  "homepage": "https://www.ember-bootstrap.com/",
  "bugs": {
    "url": "https://github.com/kaliber5/ember-bootstrap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliber5/ember-bootstrap"
  },
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "lerna-changelog",
    "docs:build": "gulp docs:build",
    "docs:serve": "gulp docs:serve",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "nodetest": "mocha node-tests --recursive",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "resolutions": {
    "ember-cli-htmlbars/semver": "~7.0.0",
    "ip-regex": "^2.1.0"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "@embroider/macros": "^0.41.0",
    "@embroider/util": "^0.41.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-debug": "^0.6.3",
    "broccoli-funnel": "^3.0.6",
    "broccoli-merge-trees": "^4.2.0",
    "chalk": "^4.1.1",
    "ember-auto-import": "^1.11.2",
    "ember-cli-babel": "^7.26.4",
    "ember-cli-build-config-editor": "0.5.1",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-version-checker": "^5.1.2",
    "ember-concurrency": "^2.1.2",
    "ember-decorators": "^6.1.0",
    "ember-element-helper": "^0.5.0",
    "ember-focus-trap": "^0.7.0",
    "ember-in-element-polyfill": "^1.0.1",
    "ember-named-blocks-polyfill": "^0.2.4",
    "ember-on-helper": "^0.1.0",
    "ember-popper": "^0.11.3",
    "ember-ref-bucket": "^3.1.0",
    "ember-render-helpers": "^0.2.0",
    "ember-style-modifier": "^0.6.0",
    "findup-sync": "^4.0.0",
    "fs-extra": "^9.1.0",
    "resolve": "^1.18.1",
    "rsvp": "^4.0.1",
    "silent-error": "^1.0.1",
    "tracked-toolbox": "^1.2.3"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.41.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.0",
    "bootstrap-sass": "^3.3.7",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.3.4",
    "chai-things": "^0.2.0",
    "ember-a11y-testing": "^4.0.7",
    "ember-cli": "~3.26.1",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-blueprint-test-helpers": "^0.19.1",
    "ember-cli-browserstack": "^1.1.0",
    "ember-cli-content-security-policy": "^2.0.0-4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-fastboot-testing": "^0.5.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-test-loader": "^3.0.0",
    "ember-cli-yuidoc": "^0.9.1",
    "ember-compatibility-helpers": "^1.2.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^5.0.0",
    "ember-sinon-qunit": "^5.0.0",
    "ember-source": "~4.0.0-beta.3",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.5.0",
    "eslint-plugin-hbs": "^1.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "execa": "^5.0.0",
    "glob": "^7.1.7",
    "gulp": "^4.0.0",
    "gulp-connect": "^5.7.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-transform": "^3.0.5",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "loader.js": "^4.7.0",
    "merge-stream": "^2.0.0",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.6.2",
    "release-it-lerna-changelog": "^3.1.0",
    "striptags": "^3.0.0"
  },
  "engines": {
    "node": "12.* || >= 14.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "changelog": {
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": "Features",
      "bug": "Bug Fixes",
      "documentation": "Documentation",
      "internal": "Internal"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=2.3.0"
    },
    "demoURL": "https://www.ember-bootstrap.com"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
