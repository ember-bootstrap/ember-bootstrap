{
  "name": "ember-bootstrap",
  "version": "4.0.0-rc.2",
  "description": "Bootstrap components for Ember.js",
  "keywords": [
    "ember-addon",
    "bootstrap"
  ],
  "homepage": "https://www.ember-bootstrap.com/",
  "bugs": {
    "url": "https://github.com/kaliber5/ember-bootstrap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliber5/ember-bootstrap"
  },
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "lerna-changelog",
    "docs:build": "gulp docs:build",
    "docs:serve": "gulp docs:serve",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "nodetest": "mocha node-tests --recursive",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "resolutions": {
    "ember-cli-htmlbars/semver": "~7.0.0",
    "ember-concurrency": "^1.0.0",
    "ip-regex": "^2.1.0"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "@embroider/macros": "^0.19.0",
    "broccoli-debug": "^0.6.3",
    "broccoli-funnel": "^3.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "broccoli-string-replace": "^0.1.2",
    "chalk": "^4.0.0",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-build-config-editor": "0.5.1",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-cli-version-checker": "^5.1.1",
    "ember-concurrency": "^1.1.7",
    "ember-decorators": "^6.1.0",
    "ember-element-helper": "^0.3.1",
    "ember-focus-trap": "^0.4.0",
    "ember-in-element-polyfill": "^1.0.0",
    "ember-on-helper": "^0.1.0",
    "ember-popper": "^0.11.2",
    "ember-ref-modifier": "^1.0.0",
    "ember-style-modifier": "^0.5.0",
    "findup-sync": "^4.0.0",
    "fs-extra": "^9.0.1",
    "resolve": "^1.17.0",
    "rsvp": "^4.0.1",
    "silent-error": "^1.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.0",
    "bootstrap-sass": "^3.3.7",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.0.0",
    "chai-things": "^0.2.0",
    "ember-a11y-testing": "^3.0.1",
    "ember-cli": "~3.17.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-blueprint-test-helpers": "^0.19.1",
    "ember-cli-browserstack": "^0.0.7",
    "ember-cli-content-security-policy": "^2.0.0-1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.2",
    "ember-cli-fastboot-testing": "^0.4.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-test-loader": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-cli-yuidoc": "^0.8.8",
    "ember-compatibility-helpers": "^1.2.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-ref-modifier": "^1.0.0",
    "ember-resolver": "^8.0.0",
    "ember-sinon": "^5.0.0",
    "ember-sinon-qunit": "^5.0.0",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.8.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-hbs": "^1.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "execa": "^2.0.0",
    "glob": "^7.1.1",
    "gulp": "^4.0.0",
    "gulp-connect": "^5.7.0",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-transform": "^3.0.5",
    "husky": ">=4",
    "lint-staged": ">=10",
    "loader.js": "^4.7.0",
    "merge-stream": "^2.0.0",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "qunit-dom": "^1.1.0",
    "release-it": "^13.5.1",
    "release-it-lerna-changelog": "^2.3.0",
    "striptags": "^3.0.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "changelog": {
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": "Features",
      "bug": "Bug Fixes",
      "documentation": "Documentation",
      "internal": "Internal"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=2.3.0"
    },
    "demoURL": "https://www.ember-bootstrap.com"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
