{
  "name": "ember-bootstrap",
  "version": "5.1.1",
  "description": "Bootstrap components for Ember.js",
  "keywords": [
    "ember-addon",
    "bootstrap"
  ],
  "homepage": "https://www.ember-bootstrap.com/",
  "bugs": {
    "url": "https://github.com/kaliber5/ember-bootstrap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliber5/ember-bootstrap"
  },
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "lerna-changelog",
    "docs:build": "gulp docs:build",
    "docs:serve": "gulp docs:serve",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "nodetest": "mocha node-tests --recursive",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "resolutions": {
    "ember-cli-htmlbars/semver": "~7.3.0"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.0",
    "@embroider/macros": "^1.0.0",
    "@embroider/util": "^1.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-debug": "^0.6.3",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "chalk": "^4.1.1",
    "ember-auto-import": "^2.4.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-build-config-editor": "0.5.1",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-version-checker": "^5.1.2",
    "ember-concurrency": "^2.1.2",
    "ember-decorators": "^6.1.0",
    "ember-element-helper": "^0.6.0",
    "ember-focus-trap": "^1.0.0",
    "ember-in-element-polyfill": "^1.0.1",
    "ember-named-blocks-polyfill": "^0.2.4",
    "ember-on-helper": "^0.1.0",
    "ember-popper-modifier": "^2.0.0",
    "ember-ref-bucket": "^4.0.0",
    "ember-render-helpers": "^0.2.0",
    "ember-style-modifier": "^0.8.0",
    "findup-sync": "^5.0.0",
    "fs-extra": "^10.0.0",
    "resolve": "^1.18.1",
    "rsvp": "^4.0.1",
    "silent-error": "^1.0.1",
    "tracked-toolbox": "^1.2.3"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.7.0",
    "@embroider/test-setup": "1.6.0",
    "babel-eslint": "10.1.0",
    "bootstrap": "5.1.3",
    "broccoli-asset-rev": "3.0.0",
    "chai": "4.3.6",
    "chai-things": "0.2.0",
    "ember-a11y-testing": "5.0.0",
    "ember-cli": "4.1.1",
    "ember-cli-app-version": "5.0.0",
    "ember-cli-blueprint-test-helpers": "0.19.2",
    "ember-cli-browserstack": "2.0.1",
    "ember-cli-content-security-policy": "2.0.3",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-fastboot": "3.3.0",
    "ember-cli-fastboot-testing": "0.6.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-cli-test-loader": "3.0.0",
    "ember-cli-yuidoc": "0.9.1",
    "ember-compatibility-helpers": "1.2.6",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-page-title": "7.0.0",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-sinon": "5.0.0",
    "ember-sinon-qunit": "6.0.0",
    "ember-source": "4.1.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "3.16.0",
    "ember-try": "2.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "10.6.1",
    "eslint-plugin-hbs": "1.0.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-qunit": "7.2.0",
    "execa": "5.1.1",
    "glob": "7.2.0",
    "gulp": "4.0.2",
    "gulp-connect": "5.7.0",
    "gulp-insert": "0.5.0",
    "gulp-rename": "2.0.0",
    "gulp-transform": "3.0.5",
    "husky": "4.3.8",
    "lint-staged": "11.2.6",
    "loader.js": "4.7.0",
    "merge-stream": "2.0.0",
    "mocha": "9.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "qunit": "2.19.1",
    "qunit-dom": "2.0.0",
    "release-it": "14.14.3",
    "release-it-lerna-changelog": "4.0.1",
    "striptags": "3.2.0",
    "webpack": "5.72.0"
  },
  "peerDependencies": {
    "ember-source": ">=3.24"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "changelog": {
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": "Features",
      "bug": "Bug Fixes",
      "documentation": "Documentation",
      "internal": "Internal"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=2.3.0"
    },
    "demoURL": "https://www.ember-bootstrap.com"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
