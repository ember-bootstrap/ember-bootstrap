{{#if inDom}}
  {{#bs-popover/element
    id=overlayId
    placement=_placement
    fade=fade
    showHelp=showHelp
    title=title
    class=class
    renderInPlace=_renderInPlace
    target=triggerTargetElement
  }}
    {{#if hasBlock}}
      {{yield}}
    {{else}}
      {{title}}
    {{/if}}
  {{/bs-popover/element}}
{{/if}}
