{{! template-lint-disable no-unbound }}
{{unbound _parentFinder}}
{{#if inDom}}
  {{#component elementComponent
    id=overlayId
    parent=this
    placement=placement
    fade=fade
    showHelp=showHelp
    title=title
    class=class
    renderInPlace=_renderInPlace
    popperTarget=triggerTargetElement
    autoPlacement=autoPlacement
    viewportElement=viewportElement
    viewportPadding=viewportPadding
  }}
    {{yield
      (hash
        close=(action "close")
      )
    }}
  {{/component}}
{{/if}}
