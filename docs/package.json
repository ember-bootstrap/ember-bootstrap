    "prettier": "2.6.2",
    "qunit": "2.19.1",
    "qunit-dom": "2.0.0",
    "sass": "1.52.3",
    "webpack": "5.72.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      ".."
    ]
  }
